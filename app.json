    "name": "SafeSusr",
    "slug": "safesus-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/safesusWithIcon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "safesus-mobile",
    "splash": {
      "image": "./assets/safesusWithIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.safesus.mobile",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "SafeSus needs your location to show nearby safety warnings and determine which safety zone you're in.",
